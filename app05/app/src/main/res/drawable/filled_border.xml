<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools" android:shape = "rectangle">
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="1dp" android:color="#F26722" />
            <solid android:color="#F26722" />
        </shape>
    </item>

    <item android:top="1dp" android:bottom="1dp" android:right="1dp" android:left="1dp" tools:ignore="StateListReachable">
        <shape android:shape="rectangle">
            <stroke android:width="1dp" android:color="#F26722" />
            <solid android:color="#F26722" />
        </shape>
    </item>


</selector>